%body
  .w-section.hero
    .w-nav.nav{"data-animation" => "default", "data-collapse" => "medium", "data-contain" => "1", "data-duration" => "400"}
      .w-container
        %a.w-nav-brand.logo
          %div LUJIA'S STUDIO
        %nav.w-nav-menu.nav-menu{:role => "navigation"}
          %a.w-nav-link.nav-link.w--current{:href => "/", :style => "max-width: 940px;"} Portfolio
          %a.w-nav-link.nav-link{:href => "/about", :style => "max-width: 940px;"} About
          %a.w-nav-link.nav-link{:href => "/contact", :style => "max-width: 940px;"} Contact
        .w-nav-button.menu-button
          .w-icon-nav-menu
      .w-nav-overlay{"data-wf-ignore" => ""}
    %header.w-container.heading-wrapper
      %h1.main-heading LONELY PLANET
      %a.w-button.button{:href => "/contact"} FIND ME
  .w-section.section
    .w-container
      %h3.section-title Latest Work
      .w-dyn-list
        .w-dyn-items.w-row
          .w-dyn-item.w-col.w-col-4
            %a.w-inline-block{:href => "/projects/music-player"}

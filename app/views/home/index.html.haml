                %img{:src => "http://uploads.webflow.com/561d9d4c436775d57a97e910/56ca37218e937d52236f3d3c_6619442629258665064.jpg"}/
          .w-dyn-item.w-col.w-col-4
            %a.w-inline-block{:href => "/projects/sports-car-app"}
              %img.project-image{:src => "https://daks2k3a4ib2z.cloudfront.net/561d9d4d629a34d57ad4ec64/561d9d4d629a34d57ad4ec86_fan.png"}
                %img{:src => "http://uploads.webflow.com/561d9d4c436775d57a97e910/56ca37218e937d52236f3d3c_6619442629258665064.jpg"}/
          .w-dyn-item.w-col.w-col-4
            %a.w-inline-block{:href => "/projects/music-app"}
              %img.project-image{:src => "https://daks2k3a4ib2z.cloudfront.net/561d9d4d629a34d57ad4ec64/561d9d4d629a34d57ad4ec87_human.jpg"}
                %img{:src => "http://uploads.webflow.com/561d9d4c436775d57a97e910/56ca37218e937d52236f3d3c_6619442629258665064.jpg"}/
          .w-dyn-item.w-col.w-col-4

  .w-section.footer
    .w-container
      .footer-logo LUJIA'S STUDIO
      %div HELLO WORLD
